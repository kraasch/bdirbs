#!/bin/bash

#> synopsis: cdshow
#> type: interactive (bash).
#> comment: print the two lists of all directory bookmarks (Permanent and temporary).

PERM=~/.config/bashdirbs/permanent
CUST=~/.config/bashdirbs/custom

function main() {
	echo "PERM BOOKMARKS"
	bdirbs_permanent
	echo "TEMP BOOKMARKS"
	if $(test -f "$CUST"); then
		cat "$CUST"
	fi
	echo "DUPLICATE BOOKMARKS"
	bdirbs_check
}

main
